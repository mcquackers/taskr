<h1>Here are your tasks</h1>
<div id="errors"></div>
<%= render "error_messages", target: @task %>
<%= form_for @task do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <br />
  <%= form.label :description %>
  <%= form.text_field :description %>
   <br />
  <%= form.submit %>
<% end %>
<h3 id="incomplete-task-header" data-list-id="incomplete-task-list">Incomplete tasks</h3>
<ul id="incomplete-task-list">
  <%= render @incomplete_tasks %>
</ul>
<h3 id="completed-task-header" data-list-id="completed-task-list">Complete tasks</h3>
<ul id="completed-task-list">
  <%= render @completed_tasks %>
</ul>
